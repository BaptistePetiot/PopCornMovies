                  <ImageView fitHeight="55.0" fitWidth="53.0" pickOnBounds="true" preserveRatio="true" styleClass="signout_logo">
                     <image>
                        <Image url="@../imgs/icons8_Sign_Out_32px.png" />
                     </image>
                  </ImageView>
               </graphic>
               <font>
                  <Font size="30.0" />
               </font>
               <padding>
                  <Insets left="45.0" />
               </padding>
               <styleClass>
                  <String fx:value="tab" />
                  <String fx:value="signout" />
               </styleClass>
            </Button>
         </children>
      </VBox>
   </children>
</Pane>
