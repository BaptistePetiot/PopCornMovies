                  <Image url="@../imgs/circle.png" />
               </image>
               <VBox.margin>
                  <Insets bottom="20.0" top="50.0" />
               </VBox.margin>
            </ImageView>
            <Label fx:id="firstNameAndLastName" text="FirstName&amp;LastName">
